function findzx(){
	$.ajax({
		 type: "POST",
		url: basePath + "vehiclezx/findzx",
		data:'postData={"page":"'+1+
		'","terminal":"'+$('#zx_terminal').val()+
		'","vehicle":"'+$('#zx_vehicle').val()+
		'"}',
		timeout : 180000,
		dataType: 'json',
		success:function(data){
			$('#zx-tabel tbody').html("");
			var date=new Date();
			var count=0;
			for (var ii = 0; ii < data.list.length; ii++) {
				var item2 = data.list[ii];
				var date3=date.getTime()-item2.UPTIME;
				if(date3<60000){
					count +=1;
				}
			}
			$("#allvehicle").html(data.list.length);
			$("#onlinevehicle").html(count);
			if(data.list.length>0){
				var num=count/data.list.length*100
				num = num.toFixed(2)
				$("#rate").html(num+"%");
			}else{
				$("#rate").html("");
			}
       		var n=0;
       		if(data.list.length>=15){
       			n=15;
       		}else{
       			n=data.list.length;
       		}
			for (var i = 0; i < n; i++) {
				var item = data.list[i];
				var date2=date.getTime()-item.UPTIME;
				var status='<div><i class="iconfont icon-duankailianjiex"></i> 不在线</div>';
				if(date2<60000){
					status='<div><i class="iconfont icon-lianjie"></i> 在线</div>';
				}
				if(item.SEATSTATUS!=''){
					var seat="";
					var arr=item.SEATSTATUS.split(",");
						for(var z = 0; z < arr.length; z++){
							if(arr[z]==1&&date2<60000){
								seat +='<i style="color: #00FF00;font-size: 22px;"  class="iconfont icon-zuoweishu"></i>';
							}else{
								seat +='<i style="color: #606060;font-size: 22px;" class="iconfont icon-zuoweishu"></i>';
							}
						}
				}
				$('<tr><td><input type="checkbox" class="checkbox"/></td><td>' + (i + 1) + '</td>' +
						'<td>' + status + '</td>' +
						'<td>' + item.VEHICLE_NO + '</td>' +
						'<td>' + item.MDT_NO + '</td>' +
						'<td>' + seat + '</td>' +
						'<td>' + (item.UPTIME==""?"":setformat1(new Date(item.UPTIME))) + '</td>' +
						'</tr>')
						.data('database', item)
						.appendTo('#zx-tabel tbody');
			}
