	private String findsql(String power_option){
		if(power_option!=null&&power_option.length()>0&&!power_option.equals("null")){		
			String[] a = power_option.split("\\|");
			String sql = "";
			if(a[0].equals("1")){
				sql += " and cha.id= '"+a[1]+"'";
			}else if(a[0].equals("2")){
				sql += " and com.id= '"+a[1]+"'";
			}else if(a[0].equals("3")){
				sql += " and cow.id= '"+a[1]+"'";
			}else if(a[0].equals("4")){
				sql += " and veh.id= '"+a[1]+"'";
			}
			return sql;
		}
		return "";
	}
